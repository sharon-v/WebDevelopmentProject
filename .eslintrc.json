{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": [0, "error", "windows"],
    "max-len": "off",
    "no-unused-vars": "warn",
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "indent": "off",
    "space-before-function-paren": "off",
    "new-cap": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "no-var": "warn",
    // 
    "eol-last": "off",
    "quotes": "off",
    "space-before-blocks": "off",
    "keyword-spacing": "off",
    "prefer-const": "off",
    "semi": "off",
    "no-trailing-spaces": "off",
    "spaced-comment": "off",
    "no-multiple-empty-lines": "off",
    "padded-blocks": "off",

    "brace-style": "off",
    "camelcase": "off",
    "semi-spacing": "off",
    "curly": "off",
    "comma-spacing": "off",
    "no-multi-spaces": "off",
    "key-spacing": "off",
    "no-invalid-this": "off",
    "computed-property-spacing": "off"


  }
}